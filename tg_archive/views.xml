<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_move_form" model="ir.ui.view">
        <field name="name">account.move.form.tc.archive</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//group[@id='header_right_group']//label[@for='invoice_date']"
                position="before"
            >
                <field name="attachment_archive_url" widget="url" />
            </xpath>
        </field>
    </record>
</odoo>
